{
  "references": [
    {
      "path": "cdk/resources/lambda/cms-content"
    }
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "noEmit": true,
    "jsx": "react",
    "rootDir": "/",
    "outDir": "dist",
    "target": "ES2018",
    "module": "commonjs",
    "lib": [
      "DOM",
      "es2016",
      "es2017.object",
      "es2017.string"
    ],
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false
  },
  "exclude": [
    "node_modules",
    "src/**/*.spec.ts",
    "cdk.out"
  ]
}
